{
  "name": "naurls.me Chrome Extension",
  "version": "0.1",
  "description": "Shorten urls with naurls.me",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "icons": { "16": "icon.png",
           "48": "icon-48.png",
          "128": "icon-128.png" },
  "permissions": [
    "http://naurls.me/*",
    "tabs"
  ]
}