{
  "name": "naurls.me extension for Google Chrome™",
  "version": "0.4",
  "description": "Shorten URLs with naurls.me from Google Chrome™",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "icons": { "16": "icon.png",
       "48": "icon-48.png",
      "128": "icon-128.png" },
  "permissions": [
    "http://naurls.me/*",
    "tabs"
  ]
}